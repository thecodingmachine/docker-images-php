blueprint:
	orbit run generate
	@for file in $$(ls Dockerfile.*); do \
		echo '# DO NOT EDIT THIS FILE : Make yours changes in /utils/Dockerfile.*.blueprint)'$$'\n'"$$(cat $${file})" > $${file}; \
	done;
	@echo '[DO NOT EDIT THIS FILE]: <> (Make yours changes in /utils/README.blueprint.md)'$$'\n'"$$(cat README.md)" > README.md